<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>servlet.UsersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>student-info</servlet-name>
    <servlet-class>servlet.DashboardServlet</servlet-class>
  </servlet>

<servlet-mapping>
<servlet-name>user</servlet-name>
<url-pattern>/login</url-pattern>
<url-pattern>/logout</url-pattern>
<url-pattern>/signup</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>student-info</servlet-name>
<url-pattern>/registration-form</url-pattern>
<url-pattern>/exam-form</url-pattern>
<url-pattern>/backpaper-form</url-pattern>
<url-pattern>/result</url-pattern>
<url-pattern>/welcome</url-pattern>

</servlet-mapping>
</web-app>